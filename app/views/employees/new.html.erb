<h1>New Employee</h1>
<div class="container">
  <%= form_tag import_employees_path, multipart: true do %>
  <%= file_field_tag :file %>
  <!--Select Company-->
  <div class="col-sm-3 form-group">
    <select name="company_id" class="selectpicker">
      <%= @companies.each do |c|  %>
      <option value="<%= c.id %>"> <%= c.name %> </option>
      <% end  %>
    </select>
  </div>
  <%= submit_tag "Import employees file" %>
  <% end %>
</div>
